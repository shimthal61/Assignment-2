q1_data <- read_csv("assignment_2_dataset_1.csv")


q1_data <- q1_data %>%
  rename(Visual_Quality = condition) %>% 
  mutate(Visual_Quality = recode(Visual_Quality,
                            "condition_a" = "Normal",
                            "condition_b" = "Degraded")) %>% 
  mutate(Visual_Quality = factor(Visual_Quality))

